{
  "name": "pos-app",
  "main": "index",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "android:pos": "expo run:android --port 8082",
    "android:build": "cd android && ./gradlew assembleRelease",
    "prebuild": "expo prebuild",
    "postprebuild": "node scripts/setup-secrets.js",
    "postinstall": "patch-package && npm run web:setup",
    "ios": "expo run:ios",
    "web": "npm run web:setup && expo start --web",
    "web:serve": "npm run web:setup && expo serve",
    "web:build": "npm run web:setup && expo export -p web",
    "web:setup": "npx setup-skia-web && mkdir -p public && cp web/static/js/canvaskit.wasm public/",
    "lint": "expo lint"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@sentry/react-native": "~7.2.0",
    "@shopify/react-native-skia": "2.2.12",
    "@tanstack/react-query": "^5.90.11",
    "@walletconnect/safe-json": "^1.0.2",
    "expo": "^54.0.23",
    "expo-application": "~7.0.7",
    "expo-asset": "~12.0.9",
    "expo-clipboard": "~8.0.7",
    "expo-constants": "~18.0.10",
    "expo-font": "~14.0.9",
    "expo-image": "~3.0.10",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.14",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.8",
    "pressto": "0.6.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.65.0",
    "react-native": "0.81.5",
    "react-native-device-info": "^14.1.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "~1.11.0",
    "react-native-mmkv": "^4.0.0",
    "react-native-nitro-modules": "^0.31.5",
    "react-native-permissions": "^5.4.4",
    "react-native-qrcode-skia": "^0.3.1",
    "react-native-reanimated": "~4.1.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-thermal-pos-printer": "^1.6.6",
    "react-native-toast-message": "^2.3.3",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.6.1",
    "uuid": "^13.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/qrcode": "^1.5.6",
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "patch-package": "^8.0.1",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2"
  },
  "overrides": {
    "hono": "4.10.3",
    "tar": "7.5.2",
    "node-forge": "1.3.2",
    "glob": "10.5.0",
    "js-yaml": "4.1.1"
  },
